{
  "name": "react_auth_service",
  "version": "1.0.0",
  "description": "React Auth Service with MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Mihai Rusu",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^3.6.0",
    "config": "^3.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "generate-password": "^1.4.2",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^5.1.6",
    "nodemailer": "^6.3.1"
  },
  "devDependencies": {
    "nodemon": "^1.17.5"
  },
  "engines": {
    "node": "12.x"
  }
}
